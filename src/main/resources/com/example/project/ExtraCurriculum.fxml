<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="com.example.project.ExtraCurriculumController"
      xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER">
    <Button text="Open Student Counseling Form" onAction="#handleOpenStudentCounselingForm"/>
    <Button text="Open Student Report Form" onAction="#handleOpenStudentReportForm"/>
    <Button text="Open Student Participation Form" onAction="#handleOpenStudentParticipationForm"/>
    <VBox fx:id="mainVBox" spacing="10" alignment="CENTER"/>
    <TextArea fx:id="outputArea" prefHeight="200" prefWidth="300"/>
</VBox>
