<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.project.ExtraCurriculumController"
      spacing="10" alignment="CENTER">
    <TextField fx:id="studentIDField" promptText="Student ID"/>
    <TextField fx:id="sportNameField" promptText="Sport Name"/>
    <TextField fx:id="sportDateField" promptText="Sport Date"/>
    <Button text="Save" onAction="#handleSaveSportButton"/>
</VBox>
